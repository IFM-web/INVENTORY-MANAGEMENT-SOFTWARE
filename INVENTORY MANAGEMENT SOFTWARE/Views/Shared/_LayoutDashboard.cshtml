            margin-bottom: 5px;
        }

        .side-nav .sidebar .nav.sub-menu .nav-item .nav-link {
            color: #B7B7B7;
        }

        .card .card-body {
            padding: 1rem 1rem;
        }

        .main-panel-one .card-title.card-title-dash {
            font-size: 14px;
            color: #B7B7B7;
            font-weight: 500;
        }

            .main-panel-one .card-title.card-title-dash a {
                text-decoration: none;
                color: #121212;
            }

        body.with-welcome-text.sidebar-icon-only a.navbar-brand.brand-logo-mini {
            display: none;
        }

        .data-input {
            background: #ECECEC;
            padding: 6px;
            border-radius: 5px;
        }

        .main-panel-one span.icon-calendars.input-group-text.calendar-icon {
            background: white;
            border: none;
            color: black;
            display: flex;
            gap: 10px;
            align-items: center;
            font-weight: 600;
            position: relative;
        }

        .icon-date {
            font-family: 'Manrope';
        }

        .datepicker.input-group {
            border: none;
            padding: 0;
            border-radius: 5px;
            overflow: hidden;
        }

        .main-panel-one span.icon-calendars.input-group-text.calendar-icon:after {
            content: "";
            position: absolute;
            height: 30px;
            border: 1px solid #ECECEC;
            right: 0;
        }

        .btn-btn {
            border: none;
            background: #D14334;
            color: white;
            width: 100%;
            border-radius: 5px;
            padding: 17px 0px;
            font-size: 15px;
        }

        /* ------------------ */


        .employee-table {
            width: 100%;
            border-collapse: collapse;
            margin: 20px 0;
            font-size: 16px;
            text-align: left;
            border-radius: 5px;
            overflow: hidden;
        }

            .employee-table thead {
                background-color: #001C35;
                color: white;
            }

            .employee-table th,
            .employee-table td {
                padding: 12px 8px;
                font-size: 13px;
                font-weight: 600;
            }

            .employee-table tbody tr:nth-child(even) {
                background-color: #F4F4F4;
            }

            .employee-table tbody tr:nth-child(odd) {
                background-color: #E6EAEE;
            }

        .employee-photo {
            width: 50px;
            height: 50px;
            border-radius: 50%;
        }

        .status {
            padding: 4px 8px;
            border-radius: 4px;
            color: white;
            font-weight: bold;
        }

            .status.in {
                background-color: #28a745;
                border-radius: 50%;
                width: 16px;
                display: flex;
                height: 16px;
                align-items: center;
                justify-content: center;
                color: white;
                font-size: 10px;
            }

        span.in-tb {
            float: left;
            padding-right: 4px;
            color: #28a745;
        }

        span.out-tb {
            float: left;
            padding-right: 4px;
            color: #dc3545;
        }

        .status.out {
            background-color: #dc3545;
            border-radius: 50%;
            width: 16px;
            display: flex;
            height: 16px;
            align-items: center;
            justify-content: center;
            color: white;
            font-size: 10px;
        }

        .status-info {
            /* display: flex; */
            gap: 6px;
            align-items: center;
        }

        .icon {
            margin-left: 8px;
        }

        /* ----------------------- */

        @@media (min-width: 992px) {
            .sidebar-icon-only .sidebar {
                width: 0;
                background: #fff;
                overflow: hidden;
            }
        } 
    </style>
