        }

        .content-wrapper {
            padding-left: 0.4rem;
            padding-right: 0.4rem;
        }

        .side-nav .sidebar ul.nav.border-nav {
            border-top: none;
        }

        .sidebar .nav:not(.sub-menu) > .nav-item > .nav-link[aria-expanded=true] {
            background: #fff;
            color: #001C35 !important;
            border-radius: 0px;
            padding-left: 5px;
        }

        .nav-link .menu-title {
            color: #001C35 !important;
        }

        .side-nav img.cross-mys {
            position: relative;
            left: 0;
        }

        .side-nav .nav-link i {
            display: block;
            color: #121212 !important;
        }

        .side-nav a.nav-link {
            border: 1px solid #B7B7B7;
